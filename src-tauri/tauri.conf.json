{
  "build": {
    "beforeBuildCommand": "quasar build -m spa",
    "beforeDevCommand": "quasar dev",
    "devPath": "https://localhost:9000",
    "distDir": "../dist/spa",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "vuk",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
        "readFile": true,
        "readDir": true,
        "exists": true
      },
      "dialog": {
        "all": false,
        "open":true,
        "save":false
      },
      "clipboard": {
        "all": false,
        "readText": true,
        "writeText": false
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.jcsj.vuk",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Vuk",
        "width": 800
      }
    ]
  }
}
