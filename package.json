{
  "name": "vuk",
  "version": "0.0.33",
  "description": "A cross-platform epub reader",
  "productName": "Vuk",
  "author": "Jean Carlo San Juan",
  "private": true,
  "scripts": {
    "test": "echo \"No test specified\" && exit 0",
    "spa": "quasar dev -m spa",
    "pwa": "quasar dev -m pwa",
    "pwa-build": "quasar build -m pwa",
    "pwa-surge": "pnpm pwa-build && cd dist/pwa/ && surge --domain https://vuk-pwa.surge.sh/",
    "pwa-serve": "pnpm pwa-build && cd dist/pwa/ && quasar serve",
    "elec": "quasar dev -m electron '--' --no-sandbox --disable-setuid-sandbox",
    "cloudflare-server": "pnpm pwa-build && cd dist && wrangler pages deploy pwa --project-name vuk",
    "elec-debug": "quasar build -m electron --debug"
  },
  "dependencies": {
    "@jcsj/arraymap": "^0.0.4",
    "@jcsj/epub": "link:D:/code/epub",
    "@jcsj/generate-selector": "^0.0.3",
    "@quasar/extras": "^1.16.9",
    "@vueuse/core": "^10.7.2",
    "browser-fs-access": "^0.35.0",
    "dexie": "^3.2.4",
    "electron-store": "^8.1.0",
    "quasar": "^2.14.3",
    "v-walker": "link:D:/code/v-walker",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.7.3",
    "@types/node": "^20.11.7",
    "@types/wicg-file-system-access": "^2023.10.4",
    "autoprefixer": "^10.4.17",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "17.1.2",
    "typescript": "^5.3.3",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  }
}
